@model IEnumerable<Projektas.Models.Discount>

@{
    ViewBag.Title = "DiscountListView";
}

<h2>DiscountListView</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Discount_amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Start_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.End_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Creator)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Discount_amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Start_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.End_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Creator)
        </td>
    </tr>
}

</table>
